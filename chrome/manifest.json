{
	"name": "kitchen_sink",
	"version": "0.1",
	"description": "a bookmark sync addon that uses dropbox",
	"browser_action": {
		"default_icon": "gui/img/icon.png",
		"popup": "menu.html"
	},
	"options_page": "options.html",
	"permissions": [
		"bookmarks",
		"notifications",
		"tabs",
		"unlimitedStorage",
		"https://api.dropbox.com/*",
		"https://api-content.dropbox.com/*",
		"http://dl.dropbox.com/*"
	],
	"background_page": "background.html"
}